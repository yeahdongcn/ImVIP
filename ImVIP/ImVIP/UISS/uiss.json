{
    "Variables": {
        "win_bg_color": "#252134",
        
        "navi_tint_color": "#ffffff",
        
        "tv_bg_color": "#f3f3f7",
        "tv_sep_color": "#d3d3ea",
        "tv_cell_title_color": "#2b1e6a",
        "tv_cell_subtitle_color": "#b5b4ba",
        "tv_cell_subtitle_hl_color": "#fefefe",
        "tv_cell_selected_bg_color": "#00cab5",
        
        "menu_tv_bg_color": "#3d3d3d",
        "menu_tv_cell_title_color": "#fefefe",
        "menu_tv_cell_selected_bg_color": "#6b58ca",
        
        "new_card_button_tint_color": "#00cab5",
        
        "poi_bg_color": "#6b58ca",
        "poi_label_color": "#fafafa",
        "poi_button_text_color": "#1dc9af",
    },
    "RSWindowBackgroundView": {
        "backgroundColor": "$win_bg_color",
    },
    "RSTitleView": {
        "UILabel": {
            "textColor": "$navi_tint_color",
        },
        "UIImageView": {
            "image": "navi_indicator",
        }
    },
    "UINavigationBar": {
        "backgroundImage:default": "navi_bg",
        "shadowImage": "",
        "tintColor": "$navi_tint_color",
        "backIndicatorImage": "navi_back",
        "backIndicatorTransitionMaskImage": "navi_back",
    },
    "RSMenuViewController": {
        "RSMenuTableHeaderView": {
            "backgroundColor": "$menu_tv_bg_color",
        },
        "UITableView": {
            "backgroundColor": "$menu_tv_bg_color",
            "RSMenuCell": {
                "backgroundColor": "$menu_tv_bg_color",
                "UILabel": {
                    "textColor": "$menu_tv_cell_title_color",
                },
                "RSMenuCellSelectedBackgroundView": {
                    "backgroundColor": "$menu_tv_cell_selected_bg_color",
                }
            }
        }
    },
    "RSCardViewController": {
        "UITableView": {
            "backgroundColor": "$tv_bg_color",
            "separatorColor":  "$tv_sep_color",
        },
        "RSCardShowButton": {
            
        },
        "RSCardDeleteButton": {
            
        }
    },
    "RSCardsViewController": {
        "UITableView": {
            "backgroundColor": "$tv_bg_color",
            "separatorColor":  "$tv_sep_color",
            "RSCardCellAccessoryView" : {
                "image:default": "cards_arrow",
                "image:highlighted": "cards_arrow_hl",
            },
            "RSCardCellTitleLabel": {
                "textColor": "$tv_cell_title_color",
            },
            "RSCardCellSubtitleLabel": {
                "textColor": "$tv_cell_subtitle_color",
                "highlightedTextColor": "$tv_cell_subtitle_hl_color",
            },
            "RSCardCellSelectedBackgroundView": {
                "backgroundColor": "$tv_cell_selected_bg_color",
            }
        },
        "UIButton": {
            "image:default": "cards_add",
        }
    },
    "RSNewCardViewController": {
        "UITableView": {
            "backgroundColor": "$tv_bg_color",
            "separatorColor":  "$tv_sep_color",
        },
        "RSNewCardLabel": {
            "textColor": "$tv_cell_title_color",
        },
        "RSNewCardButton": {
            "tintColor": "$new_card_button_tint_color",
        },
        "RSNewCardTextField": {
            
        },
    },
    "RSProfileViewController": {
        "UITableView": {
            "backgroundColor": "$tv_bg_color",
        },
        "RSProfileViewBackgroundView": {
            "UIImageView": {
                "image": "profile_bg",
            }
        },
        "RSProfileViewAvatarView": {
            "UIImageView": {
                "image": "profile_avatar_bg",
            }
        },
        "UILabel": {
            "textColor": "$navi_tint_color",
        }
    },
    "RSPOIView": {
        "backgroundColor": "$poi_bg_color",
        "UILabel": {
            "textColor": "$poi_label_color",
        },
        "UIButton": {
            "titleColor:normal": "$poi_button_text_color",
        }
    }
}